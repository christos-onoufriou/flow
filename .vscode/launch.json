{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js: debug",
      "request": "launch",
      "type": "node",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "dev"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "Local:.*(http://localhost:[0-9]+)",
        "uriFormat": "%s"
      }
    }
  ]
}
